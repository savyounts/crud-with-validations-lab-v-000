<tbody>
  <tr>
    <th>Song</th>
    <th>Release Year</th>
    <th>Artist Name</th>
    <th>Genre</th>
  </tr>

  <% @songs.each do |song| %>
    <tr>
      <td><%= song.title%></td>
      <td><%=song.release_year%></td>
      <td><%=song.artist_name%></td>
      <td><%=song.genre%></td>
    </tr>
    <% end %>
</tbody>
